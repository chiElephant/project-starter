{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type": "tsc --noEmit",
    "a:log": "sh -c 'pnpm add \"$1\" --reporter default | tee ../logs/pnpm.log' --",
    "d:log": "sh -c 'pnpm add -D \"$1\" --reporter default | tee ../logs/pnpm.log' --",
    "i:log": "pnpm install --reporter default | tee ../logs/pnpm.log"
  },
  "dependencies": {
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@tsconfig/next": "^2.0.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
